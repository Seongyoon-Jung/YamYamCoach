{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from '@/plugins/axios';\nimport store from '@/store';\nexport default {\n  name: 'SignupView',\n  // name은 index.js에 선언해놓은 이름과 똑같아야함\n  data() {\n    return {\n      result: {\n        email: '',\n        password: ''\n      },\n      click: false,\n      warning: \"\"\n    };\n  },\n  created() {},\n  methods: {\n    login() {\n      this.click = true;\n      if (this.result.email == '') {\n        this.warning = \"이메일을 입력해주세요\";\n        return;\n      }\n      if (this.result.password == '') {\n        this.warning = \"비밀번호를 입력해주세요\";\n        return;\n      }\n\n      //  is머시기 boolean 타입으로 넘어오면 json이 강제로 surveyed로 바꿔버린다\n      axios.post('/api/auth/login', this.result).then(res => {\n        store.commit('setAccount', {\n          userId: res.data.userId,\n          username: res.data.username,\n          isSurveyed: res.data.surveyed\n        });\n\n        // 설문조사를 완료했으면 홈으로\n        if (res.data.surveyed) {\n          this.$router.push({\n            name: 'HomeView'\n          });\n        }\n        //설문조사안했으면 설문조사 페이지로\n        else {\n          this.$router.push({\n            name: 'SurveyView'\n          });\n        }\n      }).catch(err => {\n        this.warning = \"이메일 또는 비밀번호가 일치하지 않습니다\";\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","store","name","data","result","email","password","click","warning","created","methods","login","post","then","res","commit","userId","username","isSurveyed","surveyed","$router","push","catch","err"],"sources":["C:\\Users\\taewo\\Desktop\\YamYamCoach\\frontend\\src\\views\\LoginView.vue"],"sourcesContent":["<template>\r\n  <div class=\"d-flex align-items-center justify-content-center min-vh-100\">\r\n    <div\r\n      class=\"card p-5 shadow-lg\"\r\n      style=\"max-width: 450px; width: 100%; border-radius: 1rem\"\r\n    >\r\n      <h3 class=\"text-center fw-bold text-dark\">로그인</h3>\r\n      <hr />\r\n\r\n      <form>\r\n        <div class=\"row g-3\">\r\n          <span class=\"text-danger\" v-show=\"click\" v-text=\"warning\"></span>\r\n          <!-- 이메일 -->\r\n          <div class=\"col-md-12\">\r\n            <div class=\"row align-items-center\">\r\n              <label for=\"email\" class=\"col-md-3 col-form-label fw-bold\"\r\n                >이메일</label\r\n              >\r\n              <div class=\"col-md-9\">\r\n                <input\r\n                  type=\"email\"\r\n                  id=\"email\"\r\n                  class=\"form-control\"\r\n                  placeholder=\"example@domain.com\"\r\n                  name=\"email\"\r\n                  v-model=\"result.email\"\r\n                />\r\n              </div>\r\n            </div>\r\n          </div>\r\n\r\n          <!-- 비밀번호 -->\r\n          <div class=\"col-md-12\">\r\n            <div class=\"row align-items-center\">\r\n              <label for=\"password\" class=\"col-md-3 col-form-label fw-bold\"\r\n                >비밀번호</label\r\n              >\r\n              <div class=\"col-md-9\">\r\n                <input\r\n                  type=\"password\"\r\n                  id=\"password\"\r\n                  class=\"form-control\"\r\n                  placeholder=\"비밀번호를 입력하세요\"\r\n                  name=\"password\"\r\n                  v-model=\"result.password\"\r\n                />\r\n              </div>\r\n            </div>\r\n          </div>\r\n\r\n          <!-- 로그인 버튼 -->\r\n          <div class=\"col-md-12 text-center mt-4\">\r\n            <button\r\n              type=\"button\"\r\n              class=\"btn btn-success w-100 mb-3\"\r\n              @click=\"login\"\r\n            >\r\n              로그인\r\n            </button>\r\n            <router-link\r\n              class=\"btn btn-outline-primary w-100 mb-3\"\r\n              to=\"/signup\"\r\n            >\r\n              회원가입\r\n            </router-link>\r\n          </div>\r\n\r\n          <!-- 아이디 / 비밀번호 찾기 -->\r\n          <div class=\"col-md-12 d-flex justify-content-between text-sm\">\r\n            <a href=\"#\">아이디 찾기</a>\r\n            <a href=\"#\">비밀번호 찾기</a>\r\n          </div>\r\n        </div>\r\n      </form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<style scoped>\r\n.card {\r\n  background-color: white;\r\n  border-radius: 1rem;\r\n}\r\n\r\ninput::placeholder {\r\n  font-size: 0.9rem;\r\n  color: #999;\r\n}\r\n</style>\r\n\r\n<script>\r\nimport axios from '@/plugins/axios';\r\nimport store from '@/store';\r\n\r\nexport default {\r\n  name: 'SignupView', // name은 index.js에 선언해놓은 이름과 똑같아야함\r\n  data() {\r\n    return {\r\n      result: {\r\n        email: '',\r\n        password: '',\r\n      },\r\n      click: false,\r\n      warning:\"\"\r\n    };\r\n  },\r\n  created() {},\r\n  methods: {\r\n    login() {\r\n      this.click = true\r\n      if (this.result.email == '') {\r\n        this.warning = \"이메일을 입력해주세요\"\r\n        return;\r\n      }\r\n\r\n      if (this.result.password == '') {\r\n        this.warning = \"비밀번호를 입력해주세요\"\r\n        return;\r\n      }\r\n\r\n      //  is머시기 boolean 타입으로 넘어오면 json이 강제로 surveyed로 바꿔버린다\r\n      axios\r\n        .post('/api/auth/login', this.result)\r\n        .then((res) => {\r\n          store.commit('setAccount', {\r\n            userId: res.data.userId,\r\n            username: res.data.username,\r\n            isSurveyed: res.data.surveyed,\r\n          });\r\n\r\n          // 설문조사를 완료했으면 홈으로\r\n          if (res.data.surveyed) {\r\n            this.$router.push({ name: 'HomeView' });\r\n          }\r\n          //설문조사안했으면 설문조사 페이지로\r\n          else {\r\n            this.$router.push({ name: 'SurveyView' });\r\n          }\r\n        })\r\n        .catch((err) => {\r\n          this.warning = \"이메일 또는 비밀번호가 일치하지 않습니다\"\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"mappings":";AA2FA,OAAOA,KAAI,MAAO,iBAAiB;AACnC,OAAOC,KAAI,MAAO,SAAS;AAE3B,eAAe;EACbC,IAAI,EAAE,YAAY;EAAE;EACpBC,IAAIA,CAAA,EAAG;IACL,OAAO;