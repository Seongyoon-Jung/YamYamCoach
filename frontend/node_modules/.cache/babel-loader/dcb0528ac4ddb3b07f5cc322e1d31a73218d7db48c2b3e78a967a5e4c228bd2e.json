{"ast":null,"code":"import { mapState } from 'vuex';\nexport default {\n  name: 'MyPage',\n  computed: {\n    ...mapState({\n      user: state => ({\n        avatarUrl: state.account.avatarUrl,\n        username: state.account.username,\n        subscriptions: state.account.subscriptions || 0,\n        level: state.account.level || 1,\n        title: state.account.title || '초보자',\n        xp: state.account.xp || 0,\n        xpProgress: state.account.xpProgress || 0,\n        coins: state.account.coins || 0\n      })\n    })\n  }\n};","map":{"version":3,"names":["mapState","name","computed","user","state","avatarUrl","account","username","subscriptions","level","title","xp","xpProgress","coins"],"sources":["C:\\Users\\SSAFY\\Desktop\\YamYamCoach\\frontend\\src\\views\\MyView.vue"],"sourcesContent":["<template>\r\n  <main class=\"mypage-page position-relative pb-5\">\r\n    <!-- Page Header -->\r\n    <section class=\"container pt-5 mt-4\">\r\n      <h2 class=\"fw-bold mb-4\">마이페이지</h2>\r\n      <div class=\"d-flex align-items-center gap-3 mb-4\">\r\n        <img\r\n          :src=\"user.avatarUrl || '/default-avatar.png'\"\r\n          alt=\"Avatar\"\r\n          class=\"rounded-circle\"\r\n          width=\"60\"\r\n          height=\"60\"\r\n        />\r\n        <div>\r\n          <h5 class=\"mb-0\">{{ user.username }}</h5>\r\n          <small class=\"text-muted\">구독 {{ user.subscriptions }}</small>\r\n        </div>\r\n        <button class=\"btn btn-outline-secondary ms-auto\">\r\n          <i class=\"bi bi-gear-fill\"></i> 개인정보 수정\r\n        </button>\r\n        <button class=\"btn btn-link text-secondary p-2\">\r\n          <i class=\"bi bi-three-dots-vertical\"></i>\r\n        </button>\r\n      </div>\r\n    </section>\r\n\r\n    <!-- Stats Cards -->\r\n    <section class=\"container mb-4\">\r\n      <div class=\"row g-3\">\r\n        <!-- Level Card -->\r\n        <div class=\"col-lg-8\">\r\n          <div class=\"card p-4 shadow-sm\">\r\n            <div class=\"d-flex align-items-center\">\r\n              <div class=\"level-icon me-3\">\r\n                <!-- 임시 헥사곤 + 물고기 아이콘 -->\r\n                <div\r\n                  class=\"hexagon d-flex align-items-center justify-content-center\"\r\n                >\r\n                  <i class=\"bi bi-fish fs-2 text-primary\"></i>\r\n                </div>\r\n              </div>\r\n              <div>\r\n                <div class=\"d-flex align-items-center mb-2\">\r\n                  <span class=\"badge bg-success me-2\">LV.{{ user.level }}</span>\r\n                  <h5 class=\"mb-0\">{{ user.title }}</h5>\r\n                </div>\r\n                <div>{{ user.xp }} XP</div>\r\n                <div class=\"progress mt-2\" style=\"height: 6px\">\r\n                  <div\r\n                    class=\"progress-bar bg-success\"\r\n                    :style=\"{ width: user.xpProgress + '%' }\"\r\n                  ></div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- Coin Card -->\r\n        <div class=\"col-lg-4\">\r\n          <div class=\"card p-4 shadow-sm text-center\">\r\n            <h6 class=\"text-muted mb-2\">내 펄</h6>\r\n            <div class=\"fs-3 fw-bold text-primary\">{{ user.coins }}</div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n\r\n    <!-- Quick Actions -->\r\n    <section class=\"container\">\r\n      <div class=\"row g-3\">\r\n        <div class=\"col-md-6\">\r\n          <button class=\"btn btn-outline-primary w-100 py-3\">\r\n            <i class=\"bi bi-calendar-check-fill me-2\"></i> 출석체크\r\n          </button>\r\n        </div>\r\n        <div class=\"col-md-6\">\r\n          <button class=\"btn btn-outline-primary w-100 py-3\">\r\n            <i class=\"bi bi-people-fill me-2\"></i> 커뮤니티 활동\r\n          </button>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  </main>\r\n</template>\r\n\r\n<script>\r\nimport { mapState } from 'vuex';\r\n\r\nexport default {\r\n  name: 'MyPage',\r\n  computed: {\r\n    ...mapState({\r\n      user: (state) => ({\r\n        avatarUrl: state.account.avatarUrl,\r\n        username: state.account.username,\r\n        subscriptions: state.account.subscriptions || 0,\r\n        level: state.account.level || 1,\r\n        title: state.account.title || '초보자',\r\n        xp: state.account.xp || 0,\r\n        xpProgress: state.account.xpProgress || 0,\r\n        coins: state.account.coins || 0,\r\n      }),\r\n    }),\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.mypage-page {\r\n  background: #f8f9fa;\r\n  min-height: 100vh;\r\n}\r\n\r\n/* floating mission button */\r\n.mission-btn {\r\n  position: absolute;\r\n  top: 1rem;\r\n  left: 50%;\r\n  transform: translateX(-50%);\r\n  z-index: 10;\r\n}\r\n\r\n/* hexagon icon */\r\n.hexagon {\r\n  width: 60px;\r\n  height: 60px;\r\n  background: #e9ecef;\r\n  clip-path: polygon(\r\n    25% 5.77%,\r\n    75% 5.77%,\r\n    100% 50%,\r\n    75% 94.23%,\r\n    25% 94.23%,\r\n    0% 50%\r\n  );\r\n  border-radius: 4px;\r\n}\r\n.level-icon .hexagon {\r\n  background: #f1f3f5;\r\n}\r\n</style>\r\n"],"mappings":"AAuFA,SAASA,QAAO,QAAS,MAAM;AAE/B,eAAe;EACbC,IAAI,EAAE,QAAQ;EACdC,QAAQ,EAAE;IACR,GAAGF,QAAQ,CAAC;MACVG,IAAI,EAAGC,KAAK,KAAM;QAChBC,SAAS,EAAED,KAAK,CAACE,OAAO,CAACD,SAAS;QAClCE,QAAQ,EAAEH,KAAK,CAACE,OAAO,CAACC,QAAQ;QAChCC,aAAa,EAAEJ,KAAK,CAACE,OAAO,CAACE,aAAY,IAAK,CAAC;QAC/CC,KAAK,EAAEL,KAAK,CAACE,OAAO,CAACG,KAAI,IAAK,CAAC;QAC/BC,KAAK,EAAEN,KAAK,CAACE,OAAO,CAACI,KAAI,IAAK,KAAK;QACnCC,EAAE,EAAEP,KAAK,CAACE,OAAO,CAACK,EAAC,IAAK,CAAC;QACzBC,UAAU,EAAER,KAAK,CAACE,OAAO,CAACM,UAAS,IAAK,CAAC;QACzCC,KAAK,EAAET,KAAK,CAACE,OAAO,CAACO,KAAI,IAAK;MAChC,CAAC;IACH,CAAC;EACH;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}